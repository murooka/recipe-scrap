{
  "name": "template-nextjs",
  "version": "0.1.0",
  "type": "module",
  "packageManager": "pnpm@9.10.0",
  "engines": {
    "node": ">=22.8.0"
  },
  "private": true,
  "scripts": {
    "dev": "run-p dev:*",
    "dev:next": "next dev",
    "dev:vitest": "vitest watch",
    "prisma": "dotenv -e .env.local -- prisma",
    "build": "next build",
    "start": "next start",
    "test": "run-s test:*",
    "test:1": "prettier --check './**/*.{js,json,ts,tsx}'",
    "test:2": "tsc --noEmit",
    "test:3": "eslint .",
    "test:4": "vitest run",
    "watch-coverage": "vitest --ui --coverage",
    "coverage": "vitest run --coverage",
    "e2e": "playwright test",
    "lint": "eslint . --fix",
    "format": "prettier -w './**/*.{js,json,ts,tsx}'",
    "prod-deploy": "PROJECT=recipe-scrap-prod pnpm run deploy",
    "deploy": "IMAGE=asia-northeast1-docker.pkg.dev/$PROJECT/cloud-run/web run-s deploy:*",
    "deploy:1": "docker build --platform linux/amd64 -t $IMAGE .",
    "deploy:2": "docker push $IMAGE:latest",
    "deploy:3": "gcloud --project $PROJECT run deploy web --image $IMAGE --platform managed --region asia-northeast1"
  },
  "dependencies": {
    "@google-cloud/storage": "7.13.0",
    "@google-cloud/vision": "4.3.2",
    "@prisma/client": "5.20.0",
    "@radix-ui/react-slot": "1.1.0",
    "class-variance-authority": "0.7.0",
    "clsx": "2.1.1",
    "cookie": "1.0.1",
    "google-auth-library": "9.14.2",
    "lucide-react": "0.453.0",
    "next": "15.0.0-rc.1",
    "openai": "4.67.3",
    "option-t": "49.2.1",
    "react": "19.0.0-rc-83825814-20241015",
    "react-dom": "19.0.0-rc-83825814-20241015",
    "tailwind-merge": "2.5.4",
    "tailwindcss-animate": "1.0.7"
  },
  "devDependencies": {
    "@eslint/eslintrc": "3.1.0",
    "@playwright/test": "1.48.0",
    "@testcontainers/postgresql": "10.13.2",
    "@testing-library/dom": "10.4.0",
    "@testing-library/react": "16.0.1",
    "@types/node": "22.7.5",
    "@types/react": "18.3.11",
    "@types/react-dom": "18.3.1",
    "@vitejs/plugin-react": "4.3.2",
    "@vitest/browser": "2.1.3",
    "@vitest/coverage-v8": "2.1.3",
    "@vitest/ui": "2.1.3",
    "dotenv-cli": "7.4.2",
    "eslint": "8.57.1",
    "eslint-config-next": "14.2.15",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-jsx-a11y": "6.10.0",
    "eslint-plugin-react": "7.37.1",
    "eslint-plugin-react-hooks": "4.6.2",
    "npm-run-all2": "6.2.3",
    "playwright": "1.48.0",
    "postcss": "8.4.47",
    "prettier": "3.3.3",
    "prettier-plugin-tailwindcss": "0.6.8",
    "prisma": "5.20.0",
    "server-only": "0.0.1",
    "tailwindcss": "3.4.13",
    "typescript": "5.4.5",
    "typescript-eslint": "8.9.0",
    "vitest": "2.1.3"
  }
}
